{
    "container": {
        "docker": {
            "image": "mesosphere/cd-demo-app:latest",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 4000,
                    "protocol": "tcp"
                }
            ]
        },
        "type": "DOCKER"
    },
    "cpus": 0.1,
    "healthChecks": [
        {
            "gracePeriodSeconds": 600,
            "intervalSeconds": 30,
            "maxConsecutiveFailures": 2,
            "portIndex": 0,
            "protocol": "TCP",
            "timeoutSeconds": 10
        }
    ],
    "id": "cd-demo-app",
    "instances": 1,
    "constraints": [["hostname", "CLUSTER", "172.16.1.21" ]],
    "labels": {
        "HAPROXY_GROUP": "external",
        "lastChangedBy": "ndachel@gmail.com"
    },
    "mem": 128,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0
    }
}
